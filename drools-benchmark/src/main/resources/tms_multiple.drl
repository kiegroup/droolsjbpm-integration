rule "TMS0" when
    Integer( $i : intValue < 1000000, intValue % 10 == 0 )
then
    insertLogical( new Integer( $i + 1 ) );
    insertLogical( new Integer( $i + 2 ) );
    insertLogical( new Integer( $i + 3 ) );
end

